package models;

import java.util.List;

public class ModelCdpp extends Model {


	public float initialValue;
	public List<Integer> initialRowValue;

    public float getInitialValue() {
        return initialValue;
    }

    public List<Integer> getInitialRowValue() {
        return initialRowValue;
    }

    public List<Port> getPorts() {
        return ports;
    }
    
    public ModelCdpp(String name, List<String> submodels, List<Link> links, List<Port> ports) {
    	super(name, submodels, links, ports);
    	
        this.name = name;
        this.submodels = submodels;
        this.links = links;
        this.ports = ports;
    }
}
